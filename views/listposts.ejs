<!DOCTYPE html>
<html>
  <head>
    <title>List Posts</title>
    <link rel="stylesheet" type="text/css" href="/main.css" />
  </head>
  <body>
    <% posts.forEach(function(post) { %>
      <div class="post">
        <h2><%= post.title %></h2>
        <p><%= post.content %></p>
        <p><%= post.username %></p>
      </div>
    <% }); %>
  </body>
</html>